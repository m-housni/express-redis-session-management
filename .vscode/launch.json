{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "node index.js",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/index.js",
            "env": {
                "NODE_ENV": "development"
            },
            "preLaunchTask": "npm run start"
        }
    ]
}